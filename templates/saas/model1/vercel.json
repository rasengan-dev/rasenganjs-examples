{
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "client/assets/$1"
        },
        {
            "src": "/(.+\\.(png|ico|svg|jpg|jpeg|gif))",
            "dest": "client/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.js"
        }
    ]
}
